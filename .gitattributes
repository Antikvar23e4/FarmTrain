* text=auto


*.unity merge=unityyamlmerge diff=unityyamlmerge text
*.prefab merge=unityyamlmerge diff=unityyamlmerge text
*.asset merge=unityyamlmerge diff=unityyamlmerge text
*.mat merge=unityyamlmerge diff=unityyamlmerge text
*.anim merge=unityyamlmerge diff=unityyamlmerge text
*.controller merge=unityyamlmerge diff=unityyamlmerge text
*.preset merge=unityyamlmerge diff=unityyamlmerge text


*.psd filter=lfs diff=lfs merge=lfs -text
